                        print(f'A = {round(180 * A / math.pi, 3)}°\na = {round(a, 3)}\nb = {round(b, 3)}\nP = {round(P, 3)}\nS = {round(S, 3)}\nma = {round(ma, 3)}, mb = {round(mb, 3)}, mc = {round(mc, 3)}\nla = {round(la, 3)}, lb = {round(lb, 3)}, lc = {round(lc, 3)}\nha = {round(ha, 3)}, hb = {round(hb, 3)}, hc = {round(hc, 3)}\nR = {round(R, 3)}\nr = {round(2 * S / P, 3)}')
